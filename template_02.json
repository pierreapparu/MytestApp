{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "virtualMachines_UiwapappvAZW02_name": {
            "defaultValue": "UiwapappvAZW02",
            "type": "String"
        },
        "disks_UiwapappvAZW02_OsDisk_1_name": {
            "defaultValue": "uiwapappvazw02-osdisk",
            "type": "String"
        },
		"disks_UiwapappvAZW02_DataDisk_0_name": {
            "defaultValue": "uiwapappvazw02-data1",
            "type": "String"
        },
        "networkInterfaces_uiwapappvazw02-nic_externalid": {
            "defaultValue": "/subscriptions/1a3c8479-0046-4a6f-a2ad-397cb9a6f931/resourceGroups/rg-svc-prd-we-appv-1/providers/Microsoft.Network/networkInterfaces/uiwapappvazw02-nic",
            "type": "String"
		},
			"networkInterfaces_uiwapappvazw02-nic_name": {
            "defaultValue": "uiwapappvazw02-nic",
            "type": "String"
        },
        "virtualNetworks_vn_prd_we_1_externalid": {
            "defaultValue": "/subscriptions/1a3c8479-0046-4a6f-a2ad-397cb9a6f931/resourceGroups/rg-vn-prd-we-1/providers/Microsoft.Network/virtualNetworks/vn-prd-we-1",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
		{
            "type": "Microsoft.Network/networkInterfaces",
            "apiVersion": "2019-11-01",
            "name": "[parameters('networkInterfaces_uiwapappvazw02-nic_name')]",
            "location": "westeurope",
            "tags": {
                "Service": "appv"
            },
            "properties": {
                "ipConfigurations": [
                    {
                        "name": "uiwapappvazw02-ipconfig1",
                        "properties": {
                            "privateIPAddress": "10.102.130.8",
                            "privateIPAllocationMethod": "Static",
                            "subnet": {
                                "id": "[concat(parameters('virtualNetworks_vn_prd_we_1_externalid'), '/subnets/sn-vn-prd-we-1-wvd-1')]"
                            },
                            "primary": true,
                            "privateIPAddressVersion": "IPv4"
                        }
                    }
                ],
                "dnsSettings": {
                    "dnsServers": []
                },
                "enableAcceleratedNetworking": true,
                "enableIPForwarding": false
            }
        },
        {
            "type": "Microsoft.Compute/virtualMachines",
            "apiVersion": "2019-07-01",
            "name": "[parameters('virtualMachines_UiwapappvAZW02_name')]",
            "location": "westeurope",
            "tags": {
                "Service": "appv"
            },
            "zones": [
                "2"
            ],
            "properties": {
                "hardwareProfile": {
                    "vmSize": "Standard_D4s_v3"
                },
                "storageProfile": { 
                    "imageReference": {
                        "publisher": "MicrosoftWindowsServer",
                        "offer": "WindowsServer",
                        "sku": "2016-Datacenter",
                        "version": "latest"
                    },
                    "osDisk": {
                        "osType": "Windows",
                        "name": "[parameters('disks_UiwapappvAZW02_OsDisk_1_name')]",
                        "createOption": "FromImage",
                        "caching": "ReadWrite",
						"managedDisk": {
                            "storageAccountType": "StandardSSD_LRS"
                            },
                        "diskSizeGB": 127
                    },
                    "dataDisks": [
					{
                        "lun": 0,
                        "name": "[parameters('disks_UiwapappvAZW02_DataDisk_0_name')]",
                        "createOption": "Empty",
                        "caching": "None",
						"managedDisk": {
                            "storageAccountType": "Premium_LRS"
                            },
						"diskSizeGB": 512,
                        "writeAcceleratorEnabled": false
                    }
					]
                },
                "osProfile": {
                    "computerName": "[parameters('virtualMachines_UiwapappvAZW02_name')]",
                    "adminUsername": "uonadmin",
					"adminPassword": "P@ssword1234567",
                    "windowsConfiguration": {
                        "provisionVMAgent": true,
                        "enableAutomaticUpdates": true
                    },
                    "secrets": [],
                    "allowExtensionOperations": true
                },
                "networkProfile": {
                    "networkInterfaces": [
                        {
                            "id": "[parameters('networkInterfaces_uiwapappvazw02-nic_externalid')]"
                        }
                    ]
                },
                "diagnosticsProfile": {
                    "bootDiagnostics": {
                        "enabled": true,
                        "storageUri": "https://sauonprdwewvdstdlrs1.blob.core.windows.net/"
                    }
                }
            }
        },
        {
            "type": "Microsoft.Compute/virtualMachines/extensions",
            "apiVersion": "2019-07-01",
            "name": "[concat(parameters('virtualMachines_UiwapappvAZW02_name'), '/AzureNetworkWatcherExtension')]",
            "location": "westeurope",
            "dependsOn": [
                "[resourceId('Microsoft.Compute/virtualMachines', parameters('virtualMachines_UiwapappvAZW02_name'))]"
            ],
            "properties": {
                "autoUpgradeMinorVersion": true,
                "publisher": "Microsoft.Azure.NetworkWatcher",
                "type": "NetworkWatcherAgentWindows",
                "typeHandlerVersion": "1.4"
            }
        }
    ]
}